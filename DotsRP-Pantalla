        ];

        // Funci칩n para elegir una imagen aleatoria
        function randomImage() {
            var randomIndex = Math.floor(Math.random() * images.length);
            document.getElementById('loading-image').src = images[randomIndex];
        }

        // Ejecutar la funci칩n al cargar la p치gina
        randomImage();
    </script>
